{
  "name": "typescript-react-electron",
  "version": "1.0.0",
  "description": "Typescript React Electron boilerplate",
  "author": "Vladislav Tupikin <mrrefactoring@yandex.ru>",
  "license": "MIT",
  "main": "electron.js",
  "scripts": {
    "develop": "npm-run-all --parallel electron build:dev",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "build:dev": "cross-env NODE_ENV=development webpack --mode development",
    "electron": "electron .",
    "tslint": "tslint ./src",
    "tslint:fix": "npm run tslint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrRefactoring/typescript-react-electron.git"
  },
  "keywords": [
    "typescript",
    "react",
    "electron",
    "webpack",
    "template",
    "boilerplate"
  ],
  "devDependencies": {
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/reactstrap": "^8.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "electron": "^5.0.5",
    "electron-react-devtools": "^0.5.3",
    "electron-reload": "^1.4.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^0.23.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2",
    "url-loader": "^2.0.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "reactstrap": "^8.0.0"
  }
}
